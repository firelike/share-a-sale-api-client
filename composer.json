{
    "name": "firelike/share-a-sale-api-client",
    "description": "PHP client for Share-A-Sale API",
    "type": "library",
    "license": "BSD-3-Clause",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.zendframework.com/"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "guzzlehttp/guzzle": "^6.2",
        "laminas/laminas-servicemanager": "^3.1",
        "laminas/laminas-validator": "^2.8",
        "laminas/laminas-mvc-console": "^1.1",
        "laminas/laminas-dependency-plugin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Firelike\\ShareASale\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Firelike\\ShareASale\\Test\\": "tests/"
        }
    }
}
