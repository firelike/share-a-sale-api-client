{
  "name": "firelike/share-a-sale-api-client",
  "description": "PHP client for Share-A-Sale API",
  "type": "library",
  "license": "BSD-3-Clause",
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.zendframework.com/"
    }
  ],
  "require": {
    "php": "^5.6",
    "guzzlehttp/guzzle": "^6.2",
    "zendframework/zend-servicemanager": "^3.1",
    "zendframework/zend-validator": "^2.8",
    "zendframework/zend-mvc-console": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7"
  },
  "autoload": {
    "psr-4": {
      "Firelike\\ShareASale\\": "src\/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Firelike\\ShareASale\\Test\\": "tests\/"
    }
  }
}
